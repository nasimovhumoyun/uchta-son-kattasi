def maxx(a,b,c):
   if a>b and b>c:
    maxx = a
   elif a>b and c>a:
       maxx = c
   elif b>a and b>c:
       maxx = b
   else:
       print("Bu sonlar o'zaro teng ")
       return 0
   return maxx

print("3 ta son kiriting ")
q = input("1-son ")
w = input("2-son ")
e = input("3-son ")
print(maxx(q,w,e))
